{
    "configurations": [
        {
            "name": "Linux (WSL)",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "__GNUC__=14",
                "__GNUC_MINOR__=2",
                "__cplusplus=202002L",
                "_GNU_SOURCE"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/gcc-14",
            "configurationProvider": "ms-vscode.cmake-tools",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": "${default}"
            }
        }
    ],
    "version": 4
}
